        "transaction_ref": {"cat": ["FX", 100000000000]},
        "related_ref": {"cat": ["SPOT", 100000000000]},
        "debit_currency": "USD",
        "debit_amount": 10000,
        "value_date": {"fake": ["date", "%Y-%m-%d"]},
        "account_number": {"cat": ["AUTH", {"fake": ["u64", 10000000, 999999999]}]},
        "dealing_desk_bic": {"fake": ["bic8"]},
        "exchange_rate": 10000,
        "contra_currency": "EUR",
        "contra_amount": 10000,
        "session_num": "0001",
        "sequence_num": "000123",
        "uetr": {"fake": ["uuid"]}
    },
    "schema": {
        "basic_header": {
            "application_id": "F",
            "service_id": "01",
            "logical_terminal": {"cat": [{"var": "sender_bic"}, "XXXX"]},
            "sender_bic": {"var": "sender_bic"},
            "session_number": {"var": "session_num"},
            "sequence_number": {"var": "sequence_num"}
        },
        "application_header": {
            "direction": "I",
            "message_type": "900",
            "destination_address": {"cat": [{"var": "receiver_bic"}, "XXXX"]},
            "receiver_bic": {"var": "receiver_bic"},
            "priority": "N"
        },
        "user_header": {
            "service_type_identifier": "001",
            "unique_end_to_end_reference": {"var": "uetr"},
            "validation_flag": "STP"
        },
        "message_type": "900",
        "fields": {
            "20": {"reference": {"var": "transaction_ref"}},
            "21": {"reference": {"var": "related_ref"}},
            "25": {"authorisation": {"var": "account_number"}},
            "13D": {
                "date": {"fake": ["date", "%y%m%d"]},
                "time": "1425",
