{
  "patterns": {
    "bic": {
      "name": "bic",
      "description": "Bank Identifier Code format",
      "regex": "^[A-Z]{4}[A-Z]{2}[A-Z0-9]{2}([A-Z0-9]{3})?$",
      "charset": {
        "alphanumeric": true,
        "ascii_printable": true
      }
    },
    "ascii_printable": {
      "name": "ascii_printable",
      "description": "ASCII printable characters",
      "charset": {
        "ascii_printable": true
      }
    },
    "alphanumeric": {
      "name": "alphanumeric",
      "description": "Alphanumeric characters only",
      "charset": {
        "alphanumeric": true
      }
    },
    "alphabetic": {
      "name": "alphabetic",
      "description": "Alphabetic characters only",
      "charset": {
        "alphabetic": true
      }
    },
    "numeric": {
      "name": "numeric",
      "description": "Numeric characters only",
      "charset": {
        "numeric": true
      }
    },
    "currency_code": {
      "name": "currency_code",
      "description": "ISO 4217 currency code format",
      "regex": "^[A-Z]{3}$",
      "charset": {
        "alphabetic": true
      }
    },
    "amount": {
      "name": "amount",
      "description": "Monetary amount format",
      "regex": "^[0-9]{1,12}(,[0-9]{1,2})?$"
    }
  },
  "fields": {
    "20": {
      "description": "Transaction Reference Number",
      "max_length": 16,
      "pattern_ref": "ascii_printable",
      "allow_empty": false
    },
    "21": {
      "description": "Related Reference",
      "max_length": 16,
      "pattern_ref": "ascii_printable",
      "allow_empty": false
    },
    "23B": {
      "description": "Bank Operation Code",
