            "32A": {
                "value_date": {"var": "value_date"},
                "currency": {"var": "debit_currency"},
                "amount": {"var": "debit_amount"}
            },
            "52A": {"bic": {"var": "dealing_desk_bic"}},
            "72": {
                "information": [
                    "/INS/FX SPOT TRANSACTION",
                    {
                        "substr": [
                            {
                                "cat": [
                                    "/EXCH/",
                                    {"var": "debit_currency"},
                                    "/",
                                    {"var": "contra_currency"}
                                ]
                            },
                            0,
                            35
                        ]
                    },
                    {"substr": [{"cat": ["/RATE/", {"var": "exchange_rate"}]}, 0, 35]},
                    {
                        "substr": [
                            {
                                "cat": [
                                    "/CTPY/",
                                    {"var": "contra_currency"},
                                    " ",
                                    {"var": "contra_amount"}
                                ]
                            },
                            0,
                            35
                        ]
                    },
                    "/DEAL/TREASURY DESK"
                ]
